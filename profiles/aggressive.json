{
  "name": "aggressive",
  "description": "Maximum performance - may use unsafe optimizations",
  "version": "1.0.0",

  "optimization": {
    "level": "O3",
    "simd_enabled": true,
    "lto_enabled": true,
    "inline_threshold": "aggressive",
    "loop_unrolling": true,
    "vectorization": true,
    "auto_vectorization": true,
    "profile_guided": false,
    "fast_math": true
  },

  "safety": {
    "mode": "minimal",
    "bounds_checking": "never",
    "integer_overflow_checks": "never",
    "null_pointer_checks": "never",
    "stack_protector": "no",
    "fortify_source": 0,
    "address_sanitizer": false,
    "undefined_behavior_sanitizer": false,
    "allow_unsafe": true
  },

  "compilation": {
    "debug_symbols": false,
    "warnings_as_errors": false,
    "extra_warnings": false,
    "pedantic": false,
    "static_analysis": false,
    "strip_symbols": true
  },

  "parallelism": {
    "enabled": true,
    "threshold_complexity": 5,
    "max_workers": 8,
    "thread_safety_checks": "never",
    "lock_free_algorithms": true
  },

  "target_languages": {
    "allowed": ["rust", "cpp", "zig", "go", "julia"],
    "preferred": "cpp",
    "blocked": ["ruby", "php", "js"],
    "reason": "C++ for maximum control and performance"
  },

  "compiler_flags": {
    "go": "-gcflags=-B",
    "cpp": "-O3 -march=native -mtune=native -flto -funroll-loops -ffast-math -DNDEBUG -s",
    "rust": "-C opt-level=3 -C target-cpu=native -C lto=fat -C embed-bitcode=yes -C codegen-units=1",
    "zig": "-Doptimize=ReleaseFast -Dcpu=native",
    "julia": "--optimize=3 --check-bounds=no --inline=yes"
  },

  "runtime": {
    "validation_mode": "basic",
    "parity_tolerance": 0.01,
    "statistical_validation": false,
    "fuzzing_enabled": false,
    "property_testing": false,
    "skip_validation": false
  },

  "build": {
    "incremental": true,
    "clean_build": false,
    "cache_enabled": true,
    "reproducible": false,
    "parallel_compilation": true
  },

  "reporting": {
    "verbose": false,
    "warnings_shown": "none",
    "performance_metrics": true,
    "safety_report": false,
    "benchmark_results": true
  }
}
